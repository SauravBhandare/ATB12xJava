			"name": "Get Booking Ids API",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Using Postbot \r",
							"\r",
							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.expect(pm.response.code).to.eql(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response time is less than 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response schema matches expected structure\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('array'); // Check that the response is an array\r",
							"\r",
							"    responseData.forEach(function(item) {\r",
							"        pm.expect(item).to.be.an('object'); // Each item should be an object\r",
							"        pm.expect(item).to.have.all.keys('bookingid', 'firstname', 'lastname', 'totalprice', 'depositpaid', 'bookingdates', 'additionalneeds'); // Check for expected keys\r",
							"\r",
							"        pm.expect(item.firstname).to.be.a('string').and.to.have.lengthOf.at.least(1); // Check firstname\r",
							"        pm.expect(item.lastname).to.be.a('string').and.to.have.lengthOf.at.least(1); // Check lastname\r",
							"        pm.expect(item.totalprice).to.be.a('number').and.to.be.at.least(0); // Check totalprice\r",
							"        pm.expect(item.depositpaid).to.be.a('boolean'); // Check depositpaid\r",
							"\r",
							"        pm.expect(item.bookingdates).to.be.an('object'); // Check bookingdates\r",
							"        pm.expect(item.bookingdates).to.have.all.keys('checkin', 'checkout'); // Check for expected keys in bookingdates\r",
							"        pm.expect(item.bookingdates.checkin).to.be.a('string'); // Check checkin\r",
							"        pm.expect(item.bookingdates.checkout).to.be.a('string'); // Check checkout\r",
							"\r",
							"        pm.expect(item.additionalneeds).to.be.a('string').or.to.be.undefined; // Check additionalneeds\r",
							"    });\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Each booking entry contains required fields\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;\r",
							"\r",
							"    responseData.forEach(function(booking) {\r",
							"        pm.expect(booking).to.have.all.keys('firstname', 'lastname', 'totalprice', 'bookingdates');\r",
							"        pm.expect(booking.firstname).to.be.a('string').that.is.not.empty;\r",
							"        pm.expect(booking.lastname).to.be.a('string').that.is.not.empty;\r",
							"        pm.expect(booking.totalprice).to.be.a('number').that.is.not.equal(0);\r",
							"        pm.expect(booking.bookingdates).to.be.an('object').that.is.not.empty;\r",
							"    });\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Total price should be a non-negative integer\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('array'); // Assuming the response is an array\r",
							"    responseData.forEach(function(booking) {\r",
							"        pm.expect(booking.totalprice).to.exist;\r",
							"        pm.expect(booking.totalprice).to.be.a('number').and.to.be.at.least(0);\r",
							"    });\r",
							"});\r",
							""
						],
